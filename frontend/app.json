{
  "expo": {
    "name": "Samsar Pro Max",
    "slug": "samsar-pro-max",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.front"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "transparent"
      },
      "package": "com.anonymous.front"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
